@model GasStantion.Models.UserFeedback
    
<a href="" class="special-a">
    <table class="news-card">
        <tr>
            <td>
                <img src="~/Images/user.png" />
            </td>
            <td class="content">
                <div>
                    @for (int i = 0; i < Model.Stars; i++)
                    {
                        <img src="~/Images/star-fill.png" class="star" />
                    }
                    @for (int i = 0; i < 5 - Model.Stars; i++)
                    {
                        <img src="~/Images/star-empty.png" class="star"/>
                    }
                </div>
                <div>
                    @Model.Comment
                </div>
            </td>
        </tr>
    </table>
</a>